    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:const="clr-namespace:Mcce22.SmartOffice.MobileApp.Constants"
    xmlns:local="clr-namespace:Mcce22.SmartOffice.MobileApp"
    xmlns:pages="clr-namespace:Mcce22.SmartOffice.MobileApp.Pages"
