{
    "SecretName": "mcce22-smart-office",
    "EndpointAddress": "https://data-ats.iot.us-east-1.amazonaws.com",
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Error",
                "System": "Error"
            }
        },
        "WriteTo": [
            {
                "Args": {
                    "OutputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
                },
                "Name": "Console"
            }
        ]
    }
}
